{
  "manifest_version": 3,
  "name": "AWS Instance Connect Clipboard",
  "version": "4.0",
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ],
  "host_permissions": [
    "https://*.console.aws.amazon.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.console.aws.amazon.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ]
}